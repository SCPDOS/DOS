    87                              <1> ;Else, here we have CF=CY.
    88 0000055A 80FC06              <1>     cmp ah, 06h ;If the call failed like in all later versions, check retcode.
    89 0000055D 7519                <1>     jne .dmcExit    ;If not med swap, just bubble the error up.
