    74 00000543 0000000000000000    <1> .tmp    dq 0
    75                              <1> .doMedCheck:
    76                              <1> ;SCP/BIOS v0.91 reports CF=NC AH=01h for changed and 
    77                              <1> ;                   CF=NC AH=00h for not changed and
    78                              <1> ;                   CF=CY AH=80h/86h for error.
    79                              <1> ;We need to turn this into an IBM BIOS style report where:
    80                              <1> ;                   CF=CY AH=06h for changed and 
    81                              <1> ;                   CF=NC AH=00h for not changed and
    82                              <1> ;                   CF=CY AH=80h/86h for error.
    83 0000054B 8F05F2FFFFFF        <1>     pop qword [.tmp]
    84 00000551 FF157FFFFFFF        <1>     call qword [i33Next]    ;Call previous handler and exit irq here.
    85 00000557 9C                  <1>     pushfq      ;Save the returned flags
