    15                                  %include "./src/Sysinit/oeminit.asm"
     1                              <1> %use masm
     2                              <1> ;SYSINIT doesnt care about the internal structure of the OEMINIT module.
     3                              <1> ;Thus, an OEM is free to arrange code and data within the OEMINIT module,
     4                              <1> ; as they please. OEMINIT is always the first module linked to in the DOS
     5                              <1> ; binary blob file and therefore an OEM can guarantee that the first byte 
     6                              <1> ; of the OEMINIT module will be the first byte executed by the machine.
     7                              <1> ;SYSINIT starts being invoked only once OEMINIT jumps to the symbol SYSENTRY.
     8                              <1> ;OEMINIT can even be an .EXE or .ELF executable if the firmware allows it, 
     9                              <1> ; as long as it can link with SYSINIT by EXPORTING and IMPORTING the right
    10                              <1> ; symbols, its ok! Also, the DOS linker script requires that the OEMINIT 
    11                              <1> ; module be the first thing in the executable file, with the default
    12                              <1> ; kernel drivers being the at the end, after the DOS, in the segment
    13                              <1> ; kDrvText and kDrvData (no kDrvBSS is allowed)!
    14                              <1> ;---------------------------------------------------------------------------;
    15                              <1> ;PUBLIC PROCEDURES needed to link with SYSINIT:                             ;
    16                              <1> ;---------------------------------------------------------------------------;
    17                              <1> ; OEMMCBINIT -> Does MCB chain building as SYSINIT doesn't know how to read ;
    18                              <1> ;   any memory maps. Thats on the OEM to parse and build for us.            ;
    19                              <1> ; OEMHALT -> If anything goes wrong during the initial phase of SYSINIT,    ;
    20                              <1> ;   it will use this routine to print a message and halt the machine.       ;
    21                              <1> ; OEMCALLBK -> Used to finalise any setup before xfring control to SHELL=   ;
    22                              <1> ;   At this point, DOS is ready to be used.                                 ;
    23                              <1> ;---------------------------------------------------------------------------;
    24                              <1> ;EXTERN VARS needed to link with SYSINIT:                                   ;
    25                              <1> ;---------------------------------------------------------------------------;
    26                              <1> ; These vars need to be initialised before jumping to SYSENTRY              ;
    27                              <1> ;---------------------------------------------------------------------------;
    28                              <1> ;FINALDOSPTR dq ?    ;Pointer to where dSeg should be loaded                ;
    29                              <1> ;FILES       db ?    ;Default number of FILES                               ;
    30                              <1> ;BUFFERS     db ?    ;Default number of BUFFERS                             ;
    31                              <1> ;DFLTDRIVE   db ?    ;Default drive number (0-25), this is the boot drive   ;
    32                              <1> ;LASTDRIVE   db ?    ;Default last drive number (0-25)                      ;
    33                              <1> ;OEMBIOS     db ?    ;Set if to use IO.SYS or clear if to use SCPBIOS.SYS   ;
    34                              <1> ;OEMDRVCHAIN dq ?    ;Pointer to the uninitialised device drivers           ;
    35                              <1> ;OEMPTR      dq ?    ;Pointer to store at biosPtr                           ;
    36                              <1> ;OEMVERSION  dd ?    ;BIOS number, to be used by drivers for id-ing         ;
    37                              <1> ;---------------------------------------------------------------------------;
    38                              <1> ; These vars are initialised by SYSINIT, to be used in OEMMCBINIT           ;
    39                              <1> ; These vars are undefined outside of OEMMCBINIT                            ;
