   212 00000185 52010402            <1>     db errDirExist, eClsOoR, eActAbt, eLocDsk
   213                              <1> ;Error 32: Network request not supported by DOS
   214 00000189 32090303            <1>     db errNoNet, eClsBadFmt, eActUsr, eLocNet
