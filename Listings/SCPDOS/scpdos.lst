   896                              <1> ;           If (driver doesn't exist or fails to init)
   897                              <1> ;               print bad driver error msg.
   898                              <1> ;       Else 
   899                              <1> ;           Bad line error msg
   900                              <1> ;       If (line terminated by CR)
   901                              <1> ;           Goto _START
   902                              <1> ; _EXIT:
   903                              <1> ;-------------------------------------------------------------------------;
