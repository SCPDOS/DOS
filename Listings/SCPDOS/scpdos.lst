    81                              <1> %assign cVar 0   
    82                              <1> %macro return 0
    83                              <1> %%_ret:
    84                              <1>     ret
    85                              <1> %assign cVar ($$ - %%_ret)
    86                              <1> %endmacro
    87                              <1> 
    88                              <1> %macro cret 1
    89                              <1> %%_base:
    90                              <1>     %if (cVar != 0 && (($$ - %%_base) - cVar) <= 126)
    91                              <1>     j%+1 short cVar
    92                              <1>     %else 
    93                              <1>     j%-1 short %%a
    94                              <1>     return
    95                              <1>     %%a:
    96                              <1>     %endif
    97                              <1> %endmacro
