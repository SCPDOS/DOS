   153                              <1> extErrTbl:
   154                              <1> ;This table contains the default values for how to respond
   155                              <1> ; to particular errors. Fields with -1 need to be filled in before
   156                              <1> ; setting variables using this table.
   157                              <1> ;Each entry in the table is 4 bytes long, and are defined as follows:
   158                              <1> ; Byte 0: Extended Error Code as a byte
   159                              <1> ; Byte 1: Error Class
   160                              <1> ; Byte 2: Error Suggested Action
   161                              <1> ; Byte 3: Error Locus
   162                              <1> ;If a byte is -1, we dont set that variable (Thus allowing the caller to set)
   163                              <1> ;
   164                              <1> ;Error 01: Invalid function number
   165 0000012C 010704FF            <1>     db errInvFnc, eClsAppFlt, eActAbt, -1   ;Locus set before call
   166                              <1> ;Error 02: File not found
   167 00000130 02080302            <1>     db errFnf, eClsNotFnd, eActUsr, eLocDsk 
   168                              <1> ;Error 03: Path not found
   169 00000134 03080302            <1>     db errPnf, eClsNotFnd, eActUsr, eLocDsk
   170                              <1> ;Error 04: Too many open handles, and no handles are left
   171 00000138 04010401            <1>     db errNhl, eClsOoR, eActAbt, eLocUnk
   172                              <1> ;Error 05: Access being denied
   173 0000013C 05030301            <1>     db errAccDen, eClsAuth, eActUsr, eLocUnk
   174                              <1> ;Error 06: Invalid File handle being provided
   175 00000140 06070401            <1>     db errBadHdl, eClsAppFlt, eActAbt, eLocUnk
   176                              <1> ;Error 07: MCB chain destroyed
   177 00000144 07070505            <1>     db errMCBbad, eClsAppFlt, eActKil, eLocMem
   178                              <1> ;Error 08: No Memory remaining
   179 00000148 08010405            <1>     db errNoMem, eClsOoR, eActAbt, eLocMem
   180                              <1> ;Error 09: Invalid MCB block Address
   181 0000014C 09070405            <1>     db errMemAddr, eClsAppFlt, eActAbt, eLocMem
   182                              <1> ;Error 0A: Bad Environment block
   183 00000150 0A070405            <1>     db errBadEnv, eClsAppFlt, eActAbt, eLocMem
   184                              <1> ;Error 0B: Data provided in a bad format
   185 00000154 0B090301            <1>     db errBadFmt, eClsBadFmt, eActUsr, eLocUnk
   186                              <1> ;Error 0C: Access Code Invalid
   187 00000158 0C070401            <1>     db errAccCde, eClsAppFlt, eActAbt, eLocUnk
   188                              <1> ;Error 0D: Error due to Invalid Data provided
   189 0000015C 0D090401            <1>     db errInvDat, eClsBadFmt, eActAbt, eLocUnk
   190                              <1> ;Error 0F: Error due to a bad drive letter being provided
   191 00000160 0F080302            <1>     db errBadDrv, eClsNotFnd, eActUsr, eLocDsk
   192                              <1> ;Error 10: Error due to attempting to delete the CWD
   193 00000164 10030302            <1>     db errDelCD, eClsAuth, eActUsr, eLocDsk
   194                              <1> ;Error 11: Error due to a unknown device being used
   195 00000168 110D0302            <1>     db errDevUnk, eClsUnk, eActUsr, eLocDsk
   196                              <1> ;Error 12: No more file handles available
   197 0000016C 12080302            <1>     db errNoFil, eClsNotFnd, eActUsr, eLocDsk
   198                              <1> ;Error 50: Network request not supported
   199 00000170 320C0302            <1>     db errNoNet, eClsClash, eActUsr, eLocDsk
   200                              <1> ;Error 20: Generic Share Violation, Sharing Resource cannot be shared
   201 00000174 200A0202            <1>     db errShrVio, eClsLocked, eActDRet, eLocDsk
   202                              <1> ;Error 21: File Locking Violation
   203 00000178 210A0202            <1>     db errLokVio, eClsLocked, eActDRet, eLocDsk
   204                              <1> ;Error 54: Too many levels of redirection error
   205 0000017C 540104FF            <1>     db errRedir, eClsOoR, eActAbt, -1
   206                              <1> ;Error 56: Bad resource password provided
   207 00000180 56030301            <1>     db errBadPass, eClsAuth, eActUsr, eLocUnk
   208                              <1> ;Error 52: Directory already exists
   209 00000184 52010402            <1>     db errDirExist, eClsOoR, eActAbt, eLocDsk
   210                              <1> ;Error 32: Network request not supported by DOS
   211 00000188 32090303            <1>     db errNoNet, eClsBadFmt, eActUsr, eLocNet
   212                              <1> ;Error 55: Trying to duplicate a redirection for a resource
   213 0000018C 550C0303            <1>     db errDupRedir, eClsClash, eActUsr, eLocNet
   214                              <1> ;Error 57: Bad parameter in request
   215 00000190 57090301            <1>     db errBadParam, eClsBadFmt, eActUsr, eLocUnk
   216                              <1> ;Error 53: Fail was returned from Int 44h
   217 00000194 530D0401            <1>     db errFI44, eClsUnk, eActAbt, eLocUnk
   218                              <1> ;Error 24: Sharing Buffer Full
   219 00000198 24010405            <1>     db errShrFul, eClsOoR, eActAbt, eLocMem
