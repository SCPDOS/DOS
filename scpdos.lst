    48 0000014F CD44                <1>     int 44h ;Call critical error handler
    49 00000151 488B25(F4000000)    <1>     mov rsp, qword [xInt44hRSP] ;Return to the stack of the function that failed
    50 00000158 C605(C7000000)00    <1>     mov byte [critErrFlag], 0   ;Clear critical error flag
    51 0000015F FB                  <1>     sti ;Reenable Interrupts
    52 00000160 C3                  <1>     ret
