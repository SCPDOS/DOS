   524 000003F6 CD41                        int 41h     ;Call DOS to print CRLF part of message
   525                                  
   526 000003F8 6681E7FF00                  and di, 00FFh   ;Zero the upper byte of DI just in case
   527 000003FD 480FB7FF                    movzx rdi, di
   528 00000401 4889FA                      mov rdx, rdi    ;Copy error code
   529 00000404 48C1E704                    shl rdi, 4  ;Multiply by 16
