    52 00000013 488B25(F4000000)    <1>     mov rsp, qword [xInt44hRSP] ;Return to the stack of the function that failed
    53 0000001A FB                  <1>     sti ;Reenable Interrupts
    54 0000001B C3                  <1>     ret
