    47 00000146 CD44                <1>     int 44h ;Call critical error handler
    48 00000148 488B25(F4000000)    <1>     mov rsp, qword [xInt44hRSP] ;Return to the stack of the function that failed
    49 0000014F FB                  <1>     sti ;Reenable Interrupts
    50 00000150 C3                  <1>     ret
